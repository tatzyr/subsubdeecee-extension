{
  "manifest_version": 3,
  "name": "SubSubDeeCee",
  "version": "1.1.1",
  "description": "Multilingual subtitles to WWDC videos",
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://developer.apple.com/videos/play/*"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "96": "icon96.png",
    "128": "icon128.png"
  }
}
